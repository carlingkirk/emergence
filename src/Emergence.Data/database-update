dotnet ef migrations add InitialCreate --project Emergence.Data --context EmergenceDbContext --startup-project "Emergence/Server/"
dotnet ef migrations add Incremental --project Emergence.Data --context EmergenceDbContext --startup-project "Emergence/Server/"
dotnet ef database update --context EmergenceDbContext --startup-project "../Emergence/Server/"
dotnet ef migrations list --context EmergenceDbContext --startup-project "../Emergence/Server/"
